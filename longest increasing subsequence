int solve(int s[],int i,int n, int prev)
{
    
    if(i>=n)
    {
        return 0;
    }
    if(s[i]>s[prev] || prev==-1)
    {
        return 1+solve(s,i+1,n,i);
    }
    else
    {
        return solve(s,i+1,n,prev);
    }
}
    int longestSubsequence(int n, int a[])
    {
       // your code here
       return solve(a,0,n,-1);
    }
};
